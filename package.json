{
  "name": "@recipher/core-services",
  "version": "0.1.0",
  "author": "Johnny Hall",
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build && cp -r config dist/config",
    "start:prod": "node dist/src/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register src/**/*.spec.ts",
    "test:e2e": "NODE_ENV=test npm run seed:test seed && mocha --file test/boot --timeout 10000 -r tsconfig-paths/register -r ts-node/register test/**/*.spec.ts",
    "db:production": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f config/database/production.json",
    "db:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f config/database/test.json",
    "db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f config/database/development.json",
    "seed:production": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -r config/database -n production.json",
    "seed:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -r config/database -n test.json",
    "seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -r config/database -n development.json",
    "seed:run": "npm run seed seed",
    "schema:drop": "npm run db schema:drop",
    "migrate:up": "npm run db migration:run",
    "migrate:down": "npm run db migration:revert",
    "db:setup": "npm run db schema:drop && npm run migrate:up && npm run seed:run",
    "pg:start": "docker-compose -f docker/pg/docker-compose.yml up -d",
    "pg:stop": "docker-compose -f docker/pg/docker-compose.yml down"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/core": "^7.0.8",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/swagger": "^4.5.2",
    "@nestjs/typeorm": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/lodash": "^4.14.134",
    "aws-sdk": "^2.480.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.5",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nconf": "^0.10.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "sql.js": "^0.5.0",
    "swagger-ui-express": "^4.1.4",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "typeorm": "^0.2.24",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.8",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.11",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "testdouble": "^3.10.0",
    "tslint": "5.11.0",
    "typeorm-seeding": "^1.6.1"
  },
  "volta": {
    "node": "8.15.1"
  }
}
